includes:
    - vendor/mglaman/phpstan-drupal/extension.neon
    - vendor/mglaman/phpstan-drupal/rules.neon
    - vendor/phpstan/phpstan-deprecation-rules/rules.neon

parameters:
    drupal:
        entityMappingFiles: []
    level: max
    paths:
        - web/modules/custom
        - web/themes/custom
        - web/profiles/custom
    reportUnmatchedIgnoredErrors: false
    tmpDir: var/phpstan
